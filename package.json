{
  "name": "toy-robot",
  "version": "1.0.0",
  "description": "Code Challenge for IRESS",
  "main": "./dist/main.js",
  "author": "Samuel C <schalela@live.com>",
  "license": "MIT",
  "private": false,
  "bin": {
