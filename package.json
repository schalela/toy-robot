  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.4.2",
    "jest": "^23.4.2",
    "jest-cucumber": "^1.0.7",
    "poi": "^10.2.9",
    "regenerator-runtime": "^0.12.0",
    "semistandard": "^12.0.1"
  },
  "jest": {
    "testMatch": [
      "**/*.steps.js",
      "**/*.test.js"
    ],
    "testURL": "http://localhost"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "figlet": "^1.2.0",
    "inquirer": "^6.0.0",
    "minimist": "^1.2.6"
  },
  "semistandard": {
    "env": [
      "jest"
    ],
    "globals": [
      "describe",
      "expect",
      "test",
      "jest"
    ]
  }
}
